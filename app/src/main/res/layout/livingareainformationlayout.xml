<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

<!--livingareainformationlayout-->

    <LinearLayout
        android:id="@+id/llTopLAI"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/color_background_light_grey"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/tvLAI"
            android:fontFamily="@font/avenir_next_semi_bold"
            android:text="@string/living_area_information"
            android:layout_gravity="center"
            android:gravity="center"
            android:drawableStart="@drawable/ic_property"
            android:textColor="@color/light_blue"
            android:textSize="@dimen/sp_16"
            app:layout_constraintEnd_toStartOf="@+id/ivSeccArrow"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />



    </LinearLayout>

    <View
        android:id="@+id/viewLAI"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_marginTop="@dimen/dp_8"
        android:background="#F7F7F7"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/llTopLAI"/>



    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/LivingAreaInformationExpand"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:elevation="@dimen/dp_2"
        android:visibility="visible"
        app:layout_constraintTop_toBottomOf="@+id/llTopLAI"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:padding="8dp">

            <!-- Header row -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="#DDDDDD"
                android:padding="8dp">

                <!-- <TextView
                     android:layout_width="0dp"
                     android:layout_height="wrap_content"
                     android:layout_weight="1"
                     android:gravity="center"
                     android:text="Room No."
                     android:textStyle="bold" />-->

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.5"
                    android:gravity="center"
                    android:text="@string/room_number_label"
                    android:textStyle="bold" />


                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.5"
                    android:gravity="center"
                    android:text="@string/roomlength"
                    android:textStyle="bold" />

<!--                <TextView-->
<!--                    android:layout_width="0dp"-->
<!--                    android:layout_height="wrap_content"-->
<!--                    android:layout_weight="1"-->
<!--                    android:gravity="center"-->
<!--                    android:text="@string/roomtype"-->
<!--                    android:textStyle="bold" />-->

<!--                <TextView-->
<!--                    android:layout_width="0dp"-->
<!--                    android:layout_height="wrap_content"-->
<!--                    android:layout_weight="1"-->
<!--                    android:gravity="center"-->
<!--                    android:text="@string/roomarea"-->
<!--                    android:textStyle="bold" />-->

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:visibility="gone"
                    android:gravity="center"
                    android:text="Area (SqFt.)"
                    android:textStyle="bold" />



                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="View"
                    android:textStyle="bold" />
            </LinearLayout>

            <!-- RecyclerView for listing rooms -->

            <ProgressBar
                android:id="@+id/progressBar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:visibility="gone"/>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:dividerHeight="1dp"
                android:scrollbars="vertical"/>


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/llDescAcademiaApproval"
                android:text="@string/select_approvals"
                android:layout_marginTop="@dimen/dp_20"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_16"
                />

            <com.google.android.material.textfield.TextInputLayout
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/avenir_next_demi_bold"
                android:hint="@string/select_approval"
                android:id="@+id/llLivingAreaInformation"
                android:layout_marginStart="@dimen/dp_3"
                android:textColorHint="@color/color_dark_green"
                app:boxStrokeColor="@color/color_dark_green"
                >



                <AutoCompleteTextView
                    android:id="@+id/SpinnerLivingAreaInformation"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:inputType="none"
                    android:textColor="@color/color_dark_green"
                    android:textSize="15sp"
                    tools:ignore="LabelFor"
                    />
            </com.google.android.material.textfield.TextInputLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/LivingAreaInformationRemarks"
                android:visibility="gone"
                android:text="@string/remarkss"
                android:layout_marginTop="@dimen/dp_20"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_16"
                />

            <EditText
                android:id="@+id/etLivingAreaInformationRemarks"
                style="@style/EditText"
                android:visibility="gone"
                android:hint="@string/remarks"
                android:background="@drawable/edittext_background"
                android:padding="12dp"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="2"
                android:id="@+id/llLivingAreaInformationBtn"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etInfraRemarks">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:id="@+id/btnLivingAreaInformationPrevious"
                    android:elevation="4dp"
                    android:text="@string/previous"
                    android:textColor="@color/color_dark_blue"
                    android:textSize="@dimen/sp_16"
                    android:background="@drawable/backgroung_overlay"
                    android:layout_margin="8dp" />
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_weight="1"
                    android:text="@string/save_and_next"
                    android:gravity="center"
                    android:id="@+id/btnLivingAreaInformationNext"
                    android:textColor="@color/white"
                    android:elevation="4dp"
                    android:textSize="@dimen/sp_16"
                    android:background="@drawable/card_background"
                    android:layout_margin="8dp" />


            </LinearLayout>
        </LinearLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>






